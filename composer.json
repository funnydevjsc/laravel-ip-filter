{
    "name": "funnydevjsc/laravel-ip-address-filter",
    "description": "Laravel package to filter bad ip address",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "FunnyDev\\IpFilter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Funny dev., Jsc",
            "email": "contact@funnydev.vn",
            "homepage": "https://github.com/funnydevjsc"
        }
    ],
    "homepage": "https://github.com/funnydevjsc/laravel-ip-address-filter",
    "keywords": ["Laravel", "ip", "filter"],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "maxmind/minfraud": "^3.5"
    },
    "require-dev": {
        "illuminate/support": "^8.83",
        "illuminate/http": "^8.83",
        "illuminate/routing": "^8.83",
        "illuminate/console": "^8.83",
        "phpunit/phpunit": "^10.5"
  },
  "autoload-dev": {
    "psr-4": {
      "FunnyDev\\IpFilter\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "IpFilterServiceProvider"
      ]
    }
  },
    "minimum-stability": "stable",
    "prefer-stable": true
}
